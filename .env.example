# Vector Database Configuration
# Copy this file to .env and fill in your values

# ====================
# OpenAI Configuration
# ====================
OPENAI_API_KEY=your_openai_api_key_here

# ====================
# Embedding Model Configuration
# ====================
# Model selection - REQUIRED
# Default: text-embedding-3-large (1536 dimensions)
# Alternative: text-embedding-3-small (512 dimensions)  
# Future: text-embedding-4-* (when released)
VECTOR_EMBEDDING_MODEL=text-embedding-3-large
VECTOR_EMBEDDING_DIMENSIONS=1536

# Advanced embedding settings - OPTIONAL
VECTOR_EMBEDDING_MAX_RETRIES=3
VECTOR_EMBEDDING_TIMEOUT=30000  # milliseconds
VECTOR_EMBEDDING_BATCH_SIZE=100  # OpenAI max batch size

# Embedding cache settings - OPTIONAL
VECTOR_EMBEDDING_CACHE_ENABLED=true
VECTOR_EMBEDDING_CACHE_TTL=86400  # 24 hours in seconds
VECTOR_EMBEDDING_CACHE_MAX_SIZE=100  # MB

# ====================
# Chunking Configuration
# ====================
# Basic chunking settings - REQUIRED
VECTOR_CHUNK_SIZE=400
VECTOR_CHUNK_OVERLAP=50

# Advanced chunking settings - OPTIONAL
VECTOR_MAX_CHUNK_SIZE=600
VECTOR_MIN_CHUNK_SIZE=100
VECTOR_HIERARCHY_LEVELS=3

# ====================
# Processing Configuration
# ====================
VECTOR_MAX_CONCURRENCY=5

# ====================
# Supabase Configuration
# ====================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key
