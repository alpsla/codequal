{
  "name": "@codequal/comparison-agent",
  "version": "1.0.0",
  "description": "Intelligent comparison agent for analyzing DeepWiki reports",
  "main": "dist/comparison-agent.js",
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "invoke:local": "serverless invoke local -f compareReports -p test/fixtures/sample-request.json",
    "offline": "serverless offline",
    "logs": "serverless logs -f compareReports -t",
    "remove": "serverless remove"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "jest": "^29.7.0",
    "serverless": "^3.35.2",
    "serverless-offline": "^13.2.0",
    "serverless-plugin-typescript": "^2.1.5",
    "serverless-webpack": "^5.13.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "*.ts",
      "!*.test.ts",
      "!dist/**",
      "!webpack.config.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "codequal",
    "comparison",
    "agent",
    "serverless",
    "aws-lambda"
  ],
  "author": "CodeQual Team",
  "license": "MIT"
}
