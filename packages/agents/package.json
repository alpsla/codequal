{
  "name": "@codequal/agents",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "test:pr:small": "npx ts-node src/standard/tests/regression/manual-pr-validator.ts https://github.com/sindresorhus/ky/pull/700",
    "test:pr:large": "DEEPWIKI_TIMEOUT=1200000 npx ts-node src/standard/tests/regression/manual-pr-validator.ts https://github.com/vercel/next.js/pull/31616",
    "test:pr": "npx ts-node src/standard/tests/regression/manual-pr-validator.ts",
    "test:regression": "jest src/standard/tests/regression/unified-regression-suite.test.ts",
    "test:regression:all": "jest src/standard/tests/regression/*.test.ts",
    "test:regression:core": "jest src/standard/tests/regression/core-functionality.test.ts",
    "test:regression:report": "jest src/standard/tests/regression/report-generation.test.ts",
    "test:regression:ai": "jest src/standard/tests/regression/ai-impact-categorization.test.ts",
    "setup": "bash ../../.claude/quick-setup.sh",
    "session": "npx ts-node src/standard/scripts/codequal-session-starter.ts",
    "session:start": "npm run session",
    "setup:port-forward": "pkill -f 'port-forward.*8001' && kubectl port-forward -n codequal-dev svc/deepwiki-api 8001:8001 &",
    "analyze": "npx ts-node src/standard/scripts/run-complete-analysis.ts"
  },
  "dependencies": {
    "@codequal/core": "0.1.0",
    "@codequal/database": "0.1.0",
    "@supabase/supabase-js": "^2.50.0",
    "@types/marked": "^5.0.2",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.5.0",
    "ioredis": "^5.6.1",
    "marked": "^16.2.0",
    "node-fetch": "^2.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.36.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
