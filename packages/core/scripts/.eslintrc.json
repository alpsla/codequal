{
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "@typescript-eslint/no-var-requires": "off",
    "no-console": "off",
    "@typescript-eslint/no-unused-vars": ["warn", {
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_"
    }]
  },
  "parserOptions": {
    "ecmaVersion": 2022
  }
}