# CodeQual Orchestrator E2E Test Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# GitHub Access (Required for PR analysis)
GITHUB_TOKEN=ghp_your_github_personal_access_token

# OpenAI API (Required if using GPT agents)
OPENAI_API_KEY=sk-your-openai-api-key

# Anthropic API (Required if using Claude agents)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key

# Google AI (Optional - for Gemini agents)
GOOGLE_API_KEY=your-google-api-key

# DeepSeek API (Optional - for DeepSeek agents)
DEEPSEEK_API_KEY=your-deepseek-api-key

# Test User Configuration (Optional - defaults provided)
TEST_USER_ID=test-user-123
TEST_USER_EMAIL=test@codequal.com
TEST_ORG_ID=test-org-123
TEST_SESSION_TOKEN=test-session-token

# Performance Testing Configuration (Optional)
MAX_CONCURRENT_AGENTS=5
AGENT_TIMEOUT_MS=120000
ENABLE_DETAILED_LOGGING=false

# Cost Control (Optional)
MAX_TOKENS_PER_AGENT=4000
MAX_COST_PER_TEST=10.00

# Vector DB Configuration (Optional - if using external vector DB)
VECTOR_DB_URL=
VECTOR_DB_API_KEY=

# Webhook Configuration (Optional - for notifications)
WEBHOOK_URL=
SLACK_WEBHOOK_URL=